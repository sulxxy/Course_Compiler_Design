Grammar

    0 $accept: selection_statement $end

    1 expression: EA

    2 statement: A ';'
    3          | B ';'
    4          | C ';'
    5          | D ';'
    6          | E ';'
    7          | selection_statement

    8 selection_statement: IF '(' expression ')' statement ELSE statement


